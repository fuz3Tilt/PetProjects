public class Kata {
	public static void main(String[] args) {
		System.out.println(rps("rock","paper"));
	}
  public static String rps(String p1, String p2) {
    if (p1==p2) {
    	//draw
    	return "draw";
    } if (p1=="scissors") {
    	if (p2=="paper") {
    		//p1 won
    		return "Player 1 won!";
    	}else {
    		//p2 won
    		return "Player 2 won!";
    	}
    }   if(p1=="paper") {
    		if(p2=="scissors") {
    			//p2 won
        		return "Player 2 won!";
    		}else {
    			//p1 won
        		return "Player 1 won!";
    		}
    	} else {
    		if(p2=="scissors") {
    			//p1 won
        		return "Player 1 won!";
    		}else {
    			//p2 won
        		return "Player 2 won!";
    		}
    	}
    
  }
}
