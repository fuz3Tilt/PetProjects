public class Kata {
	public static void main(String[] args) {
		System.out.println(rps("scissors","paper"));
	}
  public static String rps(String p1, String p2) {
    if (p1==p2) {
    	//draw
    	p1="draw";
    	return p1;
    } if (p1=="scissors") {
    	if (p2=="paper") {
    		//p1 won
    		p1="Player 1 won!";
    		return p1;
    	}else {
    		//p2 won
    		p2="Player 2 won!";
    		return p2;
    	}
    }   if(p1=="paper") {
    		if(p2=="scissors") {
    			//p2 won
    			p2="Player 2 won!";
        		return p2;
    		}else {
    			//p1 won
    			p1="Player 1 won!";
        		return p1;
    		}
    	} else {
    		if(p2=="scissors") {
    			//p1 won
    			p1="Player 1 won!";
        		return p1;
    		}else {
    			//p2 won
    			p2="Player 2 won!";
        		return p2;
    		}
    	}
    
  }
}
